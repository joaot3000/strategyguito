{
  "version": 2,
  "builds": [
    {
      "src": "autotrade.py",  // This is the entry point of your app (it might be named differently) 
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "autotrade.py"  // Route all requests to your app 
    }
  ]
}
